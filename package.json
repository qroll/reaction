{
  "name": "reaction",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sambego/storybook-state": "^1.3.6",
    "@storybook/addon-knobs": "^5.1.9",
    "immutable-setter": "^1.1.2",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "memoize-one": "^5.0.0",
    "moment": "^2.23.0",
    "nightwatch-xhr": "^0.4.7",
    "normalizr": "^3.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-lazyload": "^2.5.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.14",
    "styled-components": "^4.1.3",
    "ultimate-pagination": "^1.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:nightwatch": "cucumber-js --require nightwatch/setup.js --require nightwatch/step_definitions --format node_modules/cucumber-pretty --format json:nightwatch/reports/cucumber_report.json nightwatch",
    "test:nightwatch:report": "node nightwatch/create-e2e-html-report.js",
    "test:cypress": "cypress run",
    "test:cypress:interactive": "cypress open",
    "test:testcafe": "gherkin-testcafe --steps testcafe/**/*.js --specs testcafe/**/*.feature --browsers chrome",
    "test:codecept": "codeceptjs run --steps",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "chrome-remote-interface": "^0.27.1",
    "chromedriver": "^78.0.1",
    "codeceptjs": "^2.0.3",
    "cucumber": "^5.1.0",
    "cucumber-html-reporter": "^4.0.5",
    "cucumber-pretty": "^1.5.0",
    "cypress": "^3.1.4",
    "cypress-cucumber-preprocessor": "^1.9.1",
    "gherkin-testcafe": "^1.5.0",
    "nightwatch": "^1.2.4",
    "nightwatch-api": "^2.3.0",
    "puppeteer": "^1.11.0",
    "resemblejs": "^3.1.0",
    "selenium-standalone": "^6.16.0",
    "start-server-and-test": "^1.7.11",
    "testcafe": "^0.23.3"
  }
}
